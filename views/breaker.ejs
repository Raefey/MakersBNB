<html>
  <head>
    <meta charset="utf-8">
    <title>MOBbnb</title>
    <link rel="stylesheet" href="css/styles.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Krona+One|Lobster|Quicksand|Nunito|Dosis">
  </head>

  <body>


    <div id="breaker-rent-form">
      <form action="/booking-request" method="post">
        <label>Name</label>
        <br/>
        <input type="text" name="breaker-name" id="breaker-name" class='rounded'></input>
        <br/><br/>

        <label>Email</label>
        <br/>
        <input type="text" name="breaker-email" id="breaker-email" class='rounded'></input>
        <br/><br/>

        <label>Message</label>
        <br/>
        <textarea type="text" name="breaker-message" id="breaker-message" rows="20" cols="50" class='rounded'></textarea>
        <br/><br/>

        <label>Dates</label>
        <br/>
        <input type="text" name="breaker-dates" id="breaker-dates" class='rounded'></input>
        <br/><br/>

        <input type=submit value="RENT!" class='submit'>
      </form>
    </div>
  </body>
</html>
